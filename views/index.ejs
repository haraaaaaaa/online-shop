<%- include("includes/head.ejs") %>
</head>
<body>
    <%- include("includes/navbar.ejs") %>
    <h1>List of products</h1>
    <% if (products.length === 0) { %>
     <h1>There are no products.</h1>
    <% } else { %>
        <ul>
        <% for(const product of products) { %>
            <%- include("includes/product.ejs", {product}) %>
        <% } %>
        </ul>
    <% }%>
</body>
</html>